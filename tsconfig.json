{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  },
  "include": ["src/**/*", "sst-env.d.ts"],
  "exclude": [
    ".sst",
    ".sst/**/*",
  ]
}